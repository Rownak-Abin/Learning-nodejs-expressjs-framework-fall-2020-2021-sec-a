<!DOCTYPE html>
<html>
<head>
	<title>Job List</title>
</head>
<body>
	<h1> Job list</h1>

	<a href="/home">Back</a> | 
	<a href="/logout">logout</a> 
	<br>

	<table border="1">
		<tr>
			<td>ID</td>
			<td>Comapany</td>
			<td>Title</td>
			<td>Location</td>
			<td>Salary</td>
			<td>Actions</td>
			
		</tr>
		

		<% 
			users.forEach( function(job){ %>
			<tr>
				<td><%= job.Jid %></td>
				<td><%= job.cName %></td>
				<td><%= job.title %></td>
				<td><%= job.location %></td>
				<td><%= job.salary %></td>
				
				<td>
					<a href="/eusers/editjob/<%= job.Jid %>">Edit</a> |
					<a href="/eusers/deletejob/<%= job.Jid %>">Delete</a>
				</td>
			</tr>

		<%
			}); 
		%>
		
	</table>

</body>
</html>